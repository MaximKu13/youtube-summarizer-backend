{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "server.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/video-summary",
      "dest": "server.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
